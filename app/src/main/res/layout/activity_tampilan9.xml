<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Tampilan_9">


    <TextView
        android:id="@+id/linevoom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="LINE VOOM"
        android:textStyle="bold"
        android:textSize="25dp"
        android:layout_marginTop="15dp"/>

    <ImageView
        android:id="@+id/btnstory"
        android:layout_width="78dp"
        android:layout_height="138dp"
        android:layout_below="@id/linevoom"
        android:src="@drawable/kontak" />


    <TextView
        android:id="@+id/textstory"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/btnstory"
        android:layout_marginTop="-25dp"
        android:text="Story Anda"
        android:textSize="15dp"
        android:textStyle="bold" />

    <ImageButton
        android:id="@+id/addvoom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:src="@drawable/add2"
        android:layout_marginLeft="300dp"/>

    <ImageButton
        android:id="@+id/addperson"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:src="@drawable/person9"
        android:layout_marginLeft="350dp"/>


    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/bottom_navigation"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="14dp"
        app:menu="@menu/bottom_navigation_menu" />


</RelativeLayout>